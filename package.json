{
  "name": "Elastron",
  "version": "1.0.3",
  "description": "Full featured elasticsearch client",
  "main": "main.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start-local": "sirv public",
    "start": "electron .",
    "dist": "electron-builder",
    "release": "electron-builder --publish=onTagOrDraft"
  },
  "repository": "https://github.com/antonkorotkov/elastron-electron",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Anton Korotkov",
  "license": "MIT",
  "build": {
    "appId": "com.elastron.www",
    "mac": {
      "category": "db.development.tools",
      "icon": "public/icon.icns",
      "publish": {
        "provider": "github",
        "repo": "elastron",
        "owner": "antonkorotkov",
        "private": true
      }
    }
  },
  "dependencies": {
    "electron-log": "^4.1.1",
    "electron-updater": "^4.2.5",
    "@storeon/router": "^1.0.1",
    "@storeon/svelte": "^0.6.2",
    "axios": "^0.19.2",
    "jsoneditor": "^8.6.4",
    "local-storage": "^2.0.0",
    "lodash": "^4.17.15",
    "sirv-cli": "^0.4.4",
    "storeon": "^2.0.2"
  },
  "devDependencies": {
    "electron": "^8.2.3",
    "electron-builder": "^22.5.1",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "node-sass": "^4.13.1",
    "rollup": "^1.20.0",
    "rollup-plugin-css-only": "^2.0.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "svelte": "^3.0.0",
    "svelte-preprocess": "^3.7.1"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "arrowParens": "avoid",
    "semi": false,
    "singleQuote": true
  }
}
